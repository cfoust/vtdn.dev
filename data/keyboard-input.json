{
  "keyboard_input": {
    "__meta": {
      "category": "Keyboard & Input"
    },
    "kitty_keyboard": {
      "__compat": {
        "title": "Kitty keyboard protocol",
        "description": "Progressive enhancement",
        "doc_path": "/docs/keyboard/kitty-keyboard",
        "support": {
          "vt100": {
            "version_added": false
          },
          "xterm": {
            "version_added": false,
            "last_scanned": "2026-02-17"
          },
          "ghostty": {
            "version_added": "v1.0.0",
            "last_scanned": "2026-02-17"
          },
          "kitty": {
            "version_added": true,
            "notes": "Full progressive enhancement support",
            "last_scanned": "2026-02-17"
          },
          "wezterm": {
            "version_added": true,
            "notes": "Requires enable_kitty_keyboard config option",
            "last_scanned": "2026-02-17"
          },
          "foot": {
            "version_added": "1.10.2",
            "last_scanned": "2026-02-17"
          },
          "alacritty": {
            "version_added": "v0.13.0",
            "last_scanned": "2026-02-17"
          },
          "iterm2": {
            "version_added": "v20260216-nightly",
            "notes": "Full progressive enhancement with CSI u, push/pop stack",
            "last_scanned": "2026-02-17"
          },
          "vte": {
            "version_added": false,
            "notes": "Mode 2017 defined but not implemented",
            "last_scanned": "2026-02-17"
          },
          "windows_terminal": {
            "version_added": true,
            "notes": "Full implementation added in commit c81c66b (Feb 17, 2026). Includes Set/Query/Push/Pop operations at adaptDispatch.cpp:2073-2100",
            "last_scanned": "2026-02-17"
          },
          "konsole": {
            "version_added": false,
            "notes": "Win32-input-mode (9001) supported instead",
            "last_scanned": "2026-02-17"
          },
          "mintty": {
            "version_added": false,
            "partial_implementation": true,
            "notes": "Only modifyOtherKeys (xterm) is implemented; kitty keyboard protocol (CSI = u) sequences not found",
            "last_scanned": "2026-02-17"
          },
          "xterm_js": {
            "version_added": true,
            "notes": "Requires vtExtensions.kittyKeyboard option, added after 6.0.0",
            "last_scanned": "2026-02-17"
          },
          "cy": {
            "version_added": "v1.11.0",
            "last_scanned": "2026-02-17"
          },
          "tmux": {
            "version_added": "3.2",
            "partial_implementation": true,
            "notes": "Supports extended keys protocol (CSI 27;mods;key~ and CSI key;mods u) similar to Kitty. Initial support in 3.2, revamped in 3.5 to support both mode 1 and mode 2",
            "last_scanned": "2026-02-17"
          },
          "bobcat": {
            "version_added": false,
            "notes": "No evidence of Kitty keyboard protocol implementation found in Bobcat codebase. Keyboard handling is delegated to TerminalCtrl library with only basic VT/PC function key styles supported.",
            "last_scanned": "2026-02-18"
          },
          "putty": {
            "version_added": false,
            "last_scanned": "2026-02-18"
          },
          "contour": {
            "version_added": "v0.4.0.6245",
            "notes": "Implemented via CSIUENTER/CSIUQUERY/CSIUENHCE/CSIULEAVE at Functions.h:541-543, handled in Screen.cpp:3990-4016",
            "last_scanned": "2026-02-18"
          },
          "mlterm": {
            "version_added": false,
            "notes": "Not implemented; uses standard xterm modifyOtherKeys mode only",
            "last_scanned": "2026-02-18"
          },
          "terminology": {
            "version_added": false,
            "last_scanned": "2026-02-18"
          },
          "screen": {
            "version_added": false,
            "last_scanned": "2026-02-18"
          },
          "rxvt_unicode": {
            "version_added": false,
            "last_scanned": "2026-02-18"
          },
          "st": {
            "version_added": false,
            "last_scanned": "2026-02-18"
          },
          "zellij": {
            "version_added": null
          },
          "rio": {
            "version_added": true,
            "notes": "Implemented in commit 426e37d936 (2025-08-19) but not yet in any released version. Available only in 'nightly' tag. Supports CSI u sequences with progressive enhancement flags.",
            "last_scanned": "2026-02-18"
          },
          "tuios": {
            "version_added": false,
            "notes": "TODO comment in internal/vt/key.go:32 explicitly states 'TODO: Support Kitty, CSI u, and XTerm modifyOtherKeys'",
            "last_scanned": "2026-02-18"
          }
        },
        "status": {
          "experimental": false,
          "standard_track": false,
          "deprecated": false
        },
        "spec_refs": [
          {
            "name": "Kitty Keyboard Protocol",
            "url": "https://sw.kovidgoyal.net/kitty/keyboard-protocol/"
          }
        ]
      }
    }
  }
}
